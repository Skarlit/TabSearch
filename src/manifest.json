{
  "manifest_version": 2,

  "name": "TabSearch",
  "description": "Hopping between tabs",
  "version": "1.0.3",

  "permissions": [
    "tabs",
    "storage"
  ],

  "options_page": "options/options.html"
  ,

  "icons" :{ "32" : "icon/icon_32.png",
             "48" : "icon/icon_48.png",
             "128" : "icon/icon_128.png"
  },

  "browser_action": {
    "default_icon" : "icon/icon_48.png",
    "default_popup": "popup/searchbar.html"
  }
}