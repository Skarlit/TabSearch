{
  "manifest_version": 2,

  "name": "TabSearch",
  "description": "Hopping between tabs",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "icons" :{ "32" : "icon_32.png",
             "48" : "icon_48.png",
             "128" : "icon_128.png"
  },

  "browser_action": {
    "default_icon" : "icon_48.png",
    "default_popup": "searchbar.html"
  }
}